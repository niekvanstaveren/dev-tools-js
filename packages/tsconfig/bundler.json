{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default for bundlers",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "noEmit": true
  }
}
